@charset "utf-8";

input, select, textarea { border: 1px solid rgb(208, 208, 208); font-family: Roboto, sans-serif; font-size: 13px; }

button, input[type="reset"], input[type="button"], input[type="submit"] { padding: 0px 6px; text-align: center; white-space: pre; }

.deleted { background-color: rgb(161, 156, 166); }

.foe { background-color: rgb(255, 102, 0); }

.num_replies { float: right; }

.no_blogs { text-align: center; font-size: 1.3em; font-weight: bold; }

#replies { text-align: center; margin-top: 15px; }

#left_menu { width: 240px; vertical-align: top; }

#blog_searchform { color: rgb(255, 255, 255); margin-top: 5px; white-space: nowrap; }

body { font-size: 11pt; font-family: Arial; text-align: center; margin: 0px; padding: 0px; display: block; background-image: none; background-color: transparent; }

textarea { padding: 5px; line-height: 1.4em; }

.inputbox { width: 100%; }

a:hover { color: rgb(55, 111, 192); text-decoration: underline; }

a:visited { color: rgb(0, 102, 204); text-decoration: none; }

a:link { color: rgb(33, 136, 224); text-decoration: none; }

.cmty-post-username a:link, .cmty-post-username a:visited { color: black; }

h1 { display: block; font-weight: normal; letter-spacing: 1px; margin: 0px; font-size: 16pt; }

a h1:hover { text-decoration: underline; }

h2 { color: rgb(0, 102, 204); font-size: 10pt; font-weight: normal; margin: 0px; }

img { border: 0px none; max-width: 100%; }

#wrapper { background: url("/m/community/img/blogs/hyperion/bg.png") right center repeat-y scroll white; border-bottom: 1px solid rgb(188, 188, 188); border-left: 1px solid rgb(188, 188, 188); border-right: 1px solid rgb(188, 188, 188); margin-left: auto; margin-right: auto; padding: 0px 2px 2px; text-align: center; width: 800px; }

#header { background: none 0px 0px repeat scroll rgb(239, 239, 239); clear: both; font-family: Tahoma; font-size: 5em; line-height: 1.5em; height: auto; width: auto; overflow: auto; vertical-align: middle; white-space: nowrap; border-top: 5px solid rgb(204, 204, 204); border-bottom: 5px solid rgb(204, 204, 204); z-index: 1; }

#main { width: 530px; padding: 5px; float: left; }

div#header h1 { text-align: center; }

#content { text-align: left; }

div.entry { border-bottom: 1px dashed rgb(204, 204, 204); margin-top: 1em; overflow-y: visible; padding-bottom: 1em; }

div.entry h1 { padding: 0.1em; overflow: hidden; }

div .entrywrap, div .entrywrap-hover { background-color: rgb(222, 222, 222); border: 1px solid black; margin: 0.5em; overflow-y: visible; padding: 0.5em; height: 99%; position: relative; }

#navigation_box { background-color: rgb(0, 102, 153); border-bottom: 1px dashed rgb(255, 255, 255); padding: 2px 2px 3px; }

#navigation_box a { color: rgb(255, 255, 255); }

#left_navigation_box { float: left; text-align: left; width: auto; }

#right_navigation_box { float: right; text-align: right; width: auto; }

div.comment { clear: both; }

div.actions { float: left; font-size: 10px; padding: 0.5em; text-align: left; }

div.modactions { float: right; font-size: 10px; padding: 0.5em 0px 0px; text-align: right; }

div.cfooter div.actions { padding: 0px; }

div.modactions ul, div.actions ul { border: medium none; display: inline; list-style: outside none square; margin-left: 0px; padding-left: 0px; }

div.modactions ul li, div.actions ul li { border: medium none; display: inline; margin-left: 0px; margin-top: 0.2em; }

div.modactions ul li a, div.actions ul li a { display: inline; text-decoration: underline; }

div.comment { border-bottom: 1px solid rgb(204, 204, 204); margin-top: 1em; overflow: auto; }

div.commentwrap { height: auto; overflow: auto; padding: 0.5em 0.5em 0px; width: 97%; position: relative; }

input.helpline { display: none; }

.clear { clear: both; }

fieldset.admin dt { border-right: 1px solid rgb(204, 204, 204); float: left; padding-top: 3px; text-align: left; width: 45%; }

fieldset.admin dd { border: none; margin: 0px 0px 0px 45%; padding: 0px 0px 0px 5px; vertical-align: top; }

#side { float: right; line-height: 1.4em; margin: 0px; padding: 5px; width: 258px; }

#side .block { clear: both; padding: 1em 0px; }

#side .widget { margin-bottom: 2em; }

.widget-title { font-size: 13px; font-weight: bold; padding-bottom: 2px; }

fieldset { border: 2px groove threedface; display: block; background-color: transparent; margin: 0px 2px; padding: 0px; }

fieldset ul { list-style: outside none none; margin: 0px; padding: 0px; }

.row1 { background-color: rgb(249, 249, 249); }

.row2 { background-color: rgb(240, 240, 240); }

fieldset.submit-buttons { text-align: center; }

div.quotetitle, div.quotecontent { background-color: rgb(235, 234, 221); border-right: 1px solid rgb(219, 219, 206); border-left: 1px solid rgb(219, 219, 206); }

div.quotecontent { border-bottom: 1px solid rgb(219, 219, 206); }

div.quotetitle { border-top: 1px solid rgb(219, 219, 206); font-weight: bold; }

ul.cats li { font-size: 8pt; line-height: 24px; }

img.latexcenter { display: block; margin: auto; text-align: center; }

.hidden-label { color: blue; text-decoration: underline; cursor: pointer; }

#blog-errors { margin: 1em; border: 2px solid rgb(136, 0, 0); background-color: rgb(238, 238, 238); font-size: 16px; }

div.post-tags { margin: 5px 0px; font-weight: bold; }

a:link.post-tag, a:visited.post-tag { font-weight: normal; }

.blog-loader { background: url("/assets/images/logo-ludicrous.gif") center center no-repeat; width: 100%; height: 40px; }

.blog-scroll-outer { width: 100%; height: 100%; left: 0px; padding: 0.35em 0.625em 0.75em; }

.blog-scroll-outer .aops-scroll-inner { width: calc(100% + 26px); }

.blog-scroll-outer .aops-scroll-content { width: 221px; }

.blog-scroll-outer .aops-scroll-bar { right: 7px; height: calc(100% - 1.1em); }

.archive-month { font-size: 10pt; }

.archive-month-header { cursor: pointer; }

.archive-row { padding-bottom: 2px; padding-left: 39px; text-indent: -24px; overflow: hidden; }

.archive-month-collapsed { display: none; }

#shouts-widget fieldset.blog-shout-wrapper { height: 400px; }

.blog-shout-wrapper .aops-scroll-content { width: 208px; }

.blog-shout .aops-icon { font-size: 16px; color: rgb(153, 0, 0); margin-right: 6px; cursor: pointer; }

#shouts-widget ul { margin-block-start: 0px; padding-inline-start: 0px; display: block; margin-top: -7px; }

#shouts-widget li { padding: 0px 3px; overflow-x: hidden; }

#message { width: 247px; height: 100px; margin: 0px 2px 2px; }

#shout-helper { display: none; }

#submit { float: right; cursor: pointer; }

.blog-reply-window .cmty-posting-box-container textarea { height: 400px; }

.blog-reply-window .cmty-post-preview { height: auto; border: 1px solid rgb(170, 170, 170); }

.blog-reply-window .cmty-bbcode-list, .blog-reply-window .cmty-bbcode-text-size { display: none; }

.blog-report-indicator { position: absolute; right: 4px; top: 4px; }

#side .cmty-itembox { height: 300px; }

.blog-tags-wrapper .blog-scroll-outer { padding-left: 0.25em; }

.blog-tags-wrapper .blog-scroll-outer .aops-scroll-content { padding-right: 0px; width: 213px; }

.entry .cmty-tags-itembox-wrapper { margin-top: 5px; position: relative; }

.entry .cmty-items-input-box input { margin: 4px 0px 0px 3px; padding-left: 4px; }

.message { overflow: auto; }

#navigation_box { background-color: rgb(27, 54, 93); border-width: 0px 0px 1px; border-bottom-style: dashed; border-bottom-color: rgb(255, 255, 255); padding: 2px 2px 3px; font-size: 10pt; display: block; font-family: Arial; height: 22px; line-height: 14px; position: absolute; border-top-style: initial; border-top-color: initial; border-right-style: initial; border-right-color: initial; border-left-style: initial; border-left-color: initial; left: 0px; top: 0px; z-index: 900000; width: 100%; margin: 0px; }

#wrapper { z-index: 1; }

#navigation_box a { color: rgb(255, 255, 255); font-size: 10pt; display: inline-block; font-family: Arial; line-height: 14px; border: none; margin: 0px; padding: 0px; }

#left_navigation_box { float: left; text-align: left; color: white; width: auto; font-size: 11pt; display: block; font-family: Arial; margin: 0px; padding: 0px; position: relative; left: 0px; top: 0px; height: 22px; line-height: 14px; border: none; }

#right_navigation_box { float: right; text-align: right; width: auto; color: white; font-size: 11pt; position: relative; display: block; font-family: Arial; height: 22px; right: 0px; margin: 0px; padding: 0px; top: 0px; line-height: 14px; border: none; }

#page_error_window { padding-top: 25px; }
